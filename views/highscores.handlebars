<body>
    <div class="container mt-5">
        <h2 class="mb-4">Leaderboard</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Username</th>
                    <th scope="col">Score</th>
                </tr>
            </thead>
            <tbody>
                {{#each scores as |score|}}
                    {{> snakeScores-info score}}
                {{/each}}
            </tbody>
        </table>
    </div>
</body>